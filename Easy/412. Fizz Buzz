from typing import List

class Solution:
    __slots__ = ()
    def fizzBuzz(self, n: int) -> List[str]:
        x = []
        a, b = 3, 5
        for i in range(1, n + 1):
            a -= 1; b -= 1
            if a == 0 and b == 0:
                x.append("FizzBuzz"); a = 3; b = 5
            elif a == 0:
                x.append("Fizz"); a = 3
            elif b == 0:
                x.append("Buzz"); b = 5
            else:
                x.append(str(i))
        return x
